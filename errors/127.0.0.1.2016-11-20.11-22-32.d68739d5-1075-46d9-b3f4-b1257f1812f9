S'# -*- coding: utf-8 -*-\n# this file is released under public domain and you can use without limitations\n\n# init setutp\n#Pin setup\nMANSARDA_UP=4\nMANSARDA_DW=24\nPIANO_UP=17\nPIANO_DW=18\nTERRA_UP=22\nTERRA_DW=23\n# pin grouped\nALL_PIN=[MANSARDA_UP,MANSARDA_DW,PIANO_UP,PIANO_DW,TERRA_UP,TERRA_DW]\nPIN_UP=[MANSARDA_UP,PIANO_UP,TERRA_UP]\nPIN_DW=[MANSARDA_DW,PIANO_DW,TERRA_DW]\nmov=False\n\nif not GPIO.getmode():\n        GPIO.setmode(GPIO.BCM)\n        for pin in ALL_PIN:\n            GPIO.setup(pin,GPIO.OUT)\n            GPIO.output(pin,GPIO.LOW)\n            print \'setto tutto fermo\'\nelse:\n    print \'GPIO settato su:{}\'.format(GPIO.getmode())\n\n\n\ndef mooving():\n    global mov\n    mov = True\n    return  True\n\ndef no_mooving():\n    global  mov\n    mov=False\n    return False\n\n#pin per tapparelle tutti out a spenti\n\n\ndef stop_all():\n    for pin in ALL_PIN:\n        GPIO.output(pin, GPIO.LOW)\n        no_mooving()\n    print \'stop_all\'\n    return True\n\ndef all_up():\n    #print \'funzione tutto su\'\n    stop_all()\n    for pin in PIN_UP:\n        GPIO.output(pin, GPIO.HIGH)\n    mooving()\n    return True\n\ndef all_dw():\n    stop_all()\n    for pin in PIN_DW:\n        GPIO.output(pin, GPIO.HIGH)\n    mooving()\n    return True\n\ndef mansarda_up():\n    GPIO.output(MANSARDA_DW, GPIO.LOW)\n    GPIO.output(MANSARDA_UP, GPIO.LOW)\n    GPIO.output(MANSARDA_UP, GPIO.HIGH)\ndef mansarda_dw():\n    GPIO.output(MANSARDA_DW, GPIO.LOW)\n    GPIO.output(MANSARDA_UP, GPIO.LOW)\n    GPIO.output(MANSARDA_DW, GPIO.HIGH)\n\n\n\n\ndef test():\n    #print \'time={}\'.format(request.now)\n    #print \'schedulato={}\'.format(request.now + timed(seconds=30))\n    #scheduler.queue_task(\'stop_current\', pvars=dict(direzione=\'up\'),start_time=request.now + timed(seconds=30),timeout = 60)\n    action=request.args[0]\n    to_return=\'test\'\n    if action==\'a_up\':\n        to_return=\'Ultima azione: alzo tutte le tapparelle\'\n        all_up()\n    elif 